{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ProjectConfig",
  "type": "object",
  "required": [
    "version"
  ],
  "properties": {
    "version": {
      "type": "string",
      "maxLength": 14,
      "minLength": 5,
      "pattern": "^(?:0|[1-9]\\d*)\\.(?:0|[1-9]\\d*)\\.(?:0|[1-9]\\d*)$"
    }
  }
}